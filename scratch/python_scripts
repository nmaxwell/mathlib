from math import *
import pylab as p
import sys
sys.path.append("/workspace/mathlib/tools/python/")
from read_file import *

for k in range(1,11):
    
    print 0.1*k
    data = read_file( str(k) )
    
    p.figure(  figsize=(12, 6), dpi=300  )
    
    for n in range (12):
        p.plot( [ log10(line[n]) for line in data ] )
    
    p.savefig( str(0.1*k) + ".png")






