from math import *
import pylab as p
import sys
sys.path.append("/workspace/mathlib/tools/python/")
from read_file import *


data = read_file( "out" )

for n in range (50):
    p.plot( [ log10(line[n]) for line in data ] )

p.show()













X = [ line[0] for line in data ]
Y1 = [ line[1] for line in data ]
Y2 = [ log10(line[2]) for line in data ]

p.plot(X,Y1)
p.show()







